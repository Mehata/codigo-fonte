<!DOCTYPE html>
<html id='html'>
  <head>
      <script src="https://code.jquery.com/jquery-latest.min.js"></script>
      <script src="/chat.js"></script>
          <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.4/socket.io.js"></script>
          <% include partials/head %>        
          <% include partials/menu %>

    <meta http-equiv="Content-Type" const="text/html;charset=UTF-8" />
    <link href="https://fonts.googleapis.com/css?family=Comfortaa" rel="stylesheet" type="text/css">
  </head>
  <body>
      <section id="titulo">
        <header>
          Upload
        </header>
      </section>
     
      <section id="chatroom">
        <section id="filebox" class="filebox">
            <table>
              <% for (item of items) {%>
                <tr><td>
                <a onclick="deleteFile('<%= item %> ' );">
                  X
                </a>
                &nbsp; - &nbsp;<%= item %> &nbsp;
              </td></tr>
              <% } %>
            </table>
        </section>
    </section>

    <section id="input_zone">
      
      
      <!-- <form id='the-form-upload' action="fileupload" method="post" enctype="multipart/form-data">
        <input type="file" name="filetoupload">
        <input type="submit">
      </form> -->

      <div id="wrap">
          <div id='field' class="field">
                   <div id="inputFile"><input type="file" id="myfile" name="myfile" class="rm-input" onchange="selectedFile();"/></div>
                   <div id="fileSize"></div>
                   <div id="fileType"></div>
                   <input type="button" value="Enviar" onClick="uploadFile()" class="rm-button" />
          </div>
          <div id="percentageCalc"></div>
       </div>
    </section>
  </body>
</html>
