<!DOCTYPE html>
<html id='html'>
  <head>
      <script src="https://code.jquery.com/jquery-latest.min.js"></script>
      <script src="/chat.js"></script>
          <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.4/socket.io.js"></script>
          <% include partials/head %>        
          <% include partials/menu %>

    <meta http-equiv="Content-Type" const="text/html;charset=UTF-8" />
    <link href="https://fonts.googleapis.com/css?family=Comfortaa" rel="stylesheet" type="text/css">
    <title>Servidor - Push Notification </title>
  </head>

  <body>
      <section id="titulo">
        <header>
          <h3>Upload</h3>
        </header>
      </section>

      <section id="chatroom">
        <section id="filebox" class="filebox">
            <table>
              <% for (item of items) {%>
                <tr><td>
                <a onclick="deleteFile('<%= item %> ' );">
                  X
                </a>
                &nbsp; - &nbsp;<%= item %> &nbsp;
              </td></tr>
              <% } %>
            </table>
        </section>
    </section>

    <section id="input_zone">
      
      
      <!-- <form id='the-form-upload' action="fileupload" method="post" enctype="multipart/form-data">
        <input type="file" name="filetoupload">
        <input type="submit">
      </form> -->

      <div id="wrap">
          <div class="field">
              <ul class="options">
                 <li><input type="file" id="myfile" name="myfile" class="rm-input" onchange="selectedFile();"/></li>
                 <li><div id="fileSize"></div></li>
                 <li><div id="fileType"></div></li>
                 <li><input type="button" value="Subir Archivo" onClick="uploadFile()" class="rm-button" /></li>
              </ul>
          </div>
          <progress id="progressBar" value="0" max="100" class="rm-progress"></progress>
          <div id="percentageCalc"></div>
       </div>

    </section>
  </body>
</html>
